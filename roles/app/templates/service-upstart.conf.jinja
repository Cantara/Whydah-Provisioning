# Ubuntu upstart file, http://upstart.ubuntu.com/cookbook/
description "{{app_name}} Upstart Configuration File"
author "Stig <stig.lau@altran.com>"

start on runlevel [2345]
stop on runlevel [06]

script
    su -c "nohup /usr/bin/java {{env_vars}} -jar /home/{{username}}/{{app_name}}.jar" -s /bin/sh - {{username}}
end script
